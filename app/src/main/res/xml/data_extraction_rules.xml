<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Example: exclude specific data -->
        <exclude domain="database" path="temp/"/>
        <exclude domain="sharedpref" path="debug_settings"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Example: exclude specific data -->
        <exclude domain="database" path="cache/"/>
        <exclude domain="file" path="temp/"/>
    </device-transfer>
</data-extraction-rules>
